{
  "env": {
    "es6": true,
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "overrides": [
    {
      "files": [
        "*.svelte"
      ],
      "processor": "svelte3/svelte3",
      "rules": {
        "import/extensions": "off",
        "import/first": "off",
        "import/no-duplicates": "off",
        "import/no-extraneous-dependencies": "off",
        "import/no-mutable-exports": "off",
        "import/no-unresolved": "off",
        "import/prefer-default-export": "off"
      }
    }
  ],
  "plugins": [
    "svelte3"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
  }
}
